<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>Ejemplos Ciudadaniz</title>
        <link rel="stylesheet" type="text/css" href="/../styles/style.css">
    </head>
    <body>
        <nav>
            | <a href="/home">Inicio</a>
            | <a href="/aprov/send">Aprobar Documento</a>
            | <a href="/auth/logout">Logout</a>
            |

        </nav>
        <header>
            <h1>Bienvenido</h1>
            
        </header>
        <main>
            <p>Datos del usuario:</p>
            <ul>
                <% for (var [key, value] of Object.entries(array)) { %>
                    <% if (typeof value === 'object'){ %>
                        <li><%= key %></li>
                        <ul>
                            <% for (var [key2, value2] of Object.entries(value)) { %>
                                <li> <%= key2+": "+value2 %></li>
                            <% } %>
                        </ul>
                    <% } else { %>
                        <li><%= key+": "+value %></li>
                    <% } %>
                <% } %>
            </ul>
        </main>
        <script src="ejs.js"></script>
    </body>
</html>
